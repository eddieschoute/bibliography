@Article{Bapat2018,
  author      = {Aniruddha Bapat and Zachary Eldredge and James R. Garrison and Abhinav Deshpande and Frederic T. Chong and Alexey V. Gorshkov},
  title       = {Unitary entanglement construction in hierarchical networks},
  journal     = {Physical Review A},
  year        = {2018},
