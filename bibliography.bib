@InProceedings{Banerjee2019,
  author      = {Indranil Banerjee and Dana Richards and Igor Shinkar},
  booktitle   = {{SOFSEM} 2019: Theory and Practice of Computer Science},
  title       = {Sorting Networks on Restricted Topologies},
  doi         = {10.1007/978-3-030-10801-4_6},
